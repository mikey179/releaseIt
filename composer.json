{
    "name": "mikey179/releaseit",
    "type": "library",
    "homepage": "http://releaseit.bovigo.org/",
    "license": "BSD-3-Clause",
    "description": "Helps to create releases from current checkout",
    "require": {
        "php": "^7.0",
        "stubbles/console": "^7.0.1",
        "herrera-io/version": "^1.0",
        "hampel/json": "^1.0"
    },
    "require-dev": {
        "mikey179/vfsStream": "^1.6",
        "phpunit/phpunit": "^5.5"
    },
    "autoload": {
        "psr-0": {
            "org\\bovigo\\releaseit": "src/main/php"
        }
    },
    "bin": ["bin/releaseIt"],
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "phar": {
            "bundler": "composer"
        }
    }
}
