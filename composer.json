    "license": "BSD-3-Clause",
    "description": "Helps to create releases from current checkout",
    "require": {
        "php": ">=5.3.0",
        "stubbles/core": "~3.1",
        "stubbles/console": "~2.3",
        "herrera-io/version": "~1.0"
    },
    "require-dev": {
